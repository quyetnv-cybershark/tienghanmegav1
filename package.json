    "nodemailer": "^6.9.14",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "react-player": "^2.16.0",
    "react-toastify": "^10.0.5",
    "redux": "^5.0.1",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
